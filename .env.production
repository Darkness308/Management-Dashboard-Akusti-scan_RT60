# Production Environment Configuration
# This file is used automatically in production builds

# Application Environment
VITE_APP_ENV=production

# Application Info
VITE_APP_NAME="Management Dashboard RT60"
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION="Akusti Scan RT60 Management Dashboard"

# API Configuration
# IMPORTANT: Replace with your production API endpoint
VITE_API_BASE_URL=https://api.rt60-dashboard.com/api
VITE_API_TIMEOUT=15000

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_PWA=true
VITE_ENABLE_NOTIFICATIONS=true
VITE_ENABLE_DEBUG_MODE=false

# Performance Monitoring
VITE_ENABLE_PERFORMANCE_MONITORING=true
VITE_PERFORMANCE_SAMPLING_RATE=0.1

# Analytics - Configure your analytics service
VITE_ANALYTICS_ENABLED=true
# VITE_ANALYTICS_ID=UA-XXXXXXXXX-X

# Sentry Error Tracking - Configure if using Sentry
VITE_SENTRY_ENABLED=true
# VITE_SENTRY_DSN=https://xxxxx@sentry.io/xxxxx
VITE_SENTRY_ENVIRONMENT=production

# Storage Configuration
VITE_STORAGE_PREFIX=rt60_dashboard_
VITE_STORAGE_VERSION=1

# Export Configuration
VITE_EXPORT_MAX_FILE_SIZE=10485760
VITE_EXPORT_FORMATS=pdf,excel,word,json,txt,md

# Data Upload Configuration
VITE_UPLOAD_MAX_FILE_SIZE=5242880
VITE_UPLOAD_ALLOWED_EXTENSIONS=.csv,.xlsx,.xls,.json

# Cache Configuration
VITE_CACHE_ENABLED=true
VITE_CACHE_TTL=3600000
VITE_CACHE_MAX_SIZE=52428800

# Agent System Configuration
VITE_AGENT_SYSTEM_ENABLED=true
VITE_AGENT_SYSTEM_DEBUG=false

# PWA Configuration
VITE_PWA_CACHE_NAME=rt60-dashboard-v1
VITE_PWA_OFFLINE_PAGE=/offline.html

# Production-specific
VITE_SOURCE_MAPS=false
VITE_MINIFY=true
