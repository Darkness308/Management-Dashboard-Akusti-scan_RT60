# Development Environment Configuration
# This file is used automatically in development mode

# Application Environment
VITE_APP_ENV=development

# Application Info
VITE_APP_NAME="Management Dashboard RT60 (Dev)"
VITE_APP_VERSION=1.0.0-dev
VITE_APP_DESCRIPTION="Akusti Scan RT60 Management Dashboard - Development"

# API Configuration
VITE_API_BASE_URL=http://localhost:3000/api
VITE_API_TIMEOUT=30000

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_PWA=true
VITE_ENABLE_NOTIFICATIONS=true
VITE_ENABLE_DEBUG_MODE=true

# Performance Monitoring
VITE_ENABLE_PERFORMANCE_MONITORING=true
VITE_PERFORMANCE_SAMPLING_RATE=1.0

# Analytics - Disabled in development
VITE_ANALYTICS_ENABLED=false

# Sentry Error Tracking - Disabled in development
VITE_SENTRY_ENABLED=false
VITE_SENTRY_ENVIRONMENT=development

# Storage Configuration
VITE_STORAGE_PREFIX=rt60_dev_
VITE_STORAGE_VERSION=1

# Export Configuration
VITE_EXPORT_MAX_FILE_SIZE=10485760
VITE_EXPORT_FORMATS=pdf,excel,word,json,txt,md

# Data Upload Configuration
VITE_UPLOAD_MAX_FILE_SIZE=5242880
VITE_UPLOAD_ALLOWED_EXTENSIONS=.csv,.xlsx,.xls,.json

# Cache Configuration
VITE_CACHE_ENABLED=true
VITE_CACHE_TTL=1800000
VITE_CACHE_MAX_SIZE=52428800

# Agent System Configuration
VITE_AGENT_SYSTEM_ENABLED=true
VITE_AGENT_SYSTEM_DEBUG=true

# PWA Configuration
VITE_PWA_CACHE_NAME=rt60-dashboard-dev-v1
VITE_PWA_OFFLINE_PAGE=/offline.html

# Development-specific
VITE_SHOW_DEV_TOOLS=true
VITE_HOT_RELOAD=true
